<ul class="pagination pagination-lg justify-content-center pagination-styles">
    <% for( let i = 1; i <= numberOfPages; i++ ) { %>
		
        <li class="page-item">
            <a href="?page=<%= i %>" class="page-link <% if (current == i) { %>
                active 
            <% } %>"><%= i %></a>
            </li>

    <% } %>
</ul>




